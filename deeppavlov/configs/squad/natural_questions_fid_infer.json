{
  "chainer": {
    "in": ["question", "contexts"],
    "pipe": [
      {
        "class_name": "torch_transformers_fid_preprocessor",
        "vocab_file": "{TRANSFORMER}",
        "max_seq_length": 512,
        "answer_maxlength" : 20,
        "in": ["question", "contexts"],
        "out": ["input_ids", "attention_mask"]
      },
      {
        "class_name": "torch_generative_qa_fid",
        "pretrained_transformer": "{TRANSFORMER}",
        "save_path": "{MODEL_PATH}/model",
        "load_path": "{MODEL_PATH}/model",
        "optimizer": "AdamW",
        "optimizer_parameters": {
          "lr": 1e-04,
          "weight_decay": 0.01,
          "betas": [0.9, 0.999],
          "eps": 1e-06
        },
        "learning_rate_drop_patience": 2,
        "learning_rate_drop_div": 2.0,
        "generate_max_length" : 20,
        "in": ["input_ids", "attention_mask"],
        "out": ["answer"]
      }
    ],
    "out": ["answer"]
  },
  "metadata": {
    "variables": {
      "TRANSFORMER": "t5-base",
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "MODEL_PATH": "{MODELS_PATH}/generative_qa/fid/natural_questions"
    }
  }
}
