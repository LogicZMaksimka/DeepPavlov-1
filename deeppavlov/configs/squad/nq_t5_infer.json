{
  "chainer": {
    "in": ["question", "contexts"],
    "pipe": [
      {
        "class_name": "torch_transformers_generative_qa_preprocessor",
        "vocab_file": "{TRANSFORMER}",
        "max_seq_length": 512,
        "answer_maxlength": 20,
        "in": ["question", "contexts"],
        "out": ["input_ids", "attention_mask"]
      },
      {
        "class_name": "torch_generative_qa_t5",
        "pretrained_transformer": "{TRANSFORMER}",
        "save_path": "{MODEL_PATH}/model",
        "load_path": "{MODEL_PATH}/model",
        "generate_max_length" : 20,
        "in": ["input_ids", "attention_mask"],
        "out": ["answer"]
      }
    ],
    "out": ["answer"]
  },
  "metadata": {
    "variables": {
      "TRANSFORMER": "t5-base",
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "MODEL_PATH": "{MODELS_PATH}/generative_qa/t5/natural_questions"
    }
  },
  "download": [
    {
      "url": "http://files.deeppavlov.ai/deeppavlov_data/generative_question_answering//en_kbqa_nq_t5/model.pth.tar",
      "subdir": "{MODEL_PATH}"
    }
  ]
}
