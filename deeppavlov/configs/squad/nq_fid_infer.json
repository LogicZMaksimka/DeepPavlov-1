{
  "chainer": {
    "in": ["question", "contexts"],
    "pipe": [
      {
        "class_name": "torch_transformers_fid_preprocessor",
        "vocab_file": "{TRANSFORMER}",
        "max_seq_length": 512,
        "answer_maxlength" : 20,
        "in": ["question", "contexts"],
        "out": ["input_ids", "attention_mask"]
      },
      {
        "class_name": "torch_generative_qa_fid",
        "pretrained_transformer": "{TRANSFORMER}",
        "save_path": "{MODEL_PATH}",
        "load_path": "{MODEL_PATH}",
        "generate_max_length" : 20,
        "in": ["input_ids", "attention_mask"],
        "out": ["answer"]
      }
    ],
    "out": ["answer"]
  },
  "metadata": {
    "variables": {
      "TRANSFORMER": "t5-base",
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "MODEL_PATH": "{MODELS_PATH}/generative_qa/fid/natural_questions"
    },
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/kbqa/models/generative_qa/fusion_in_decoder/natural_questions/config.json",
        "subdir": "{MODEL_PATH}"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/kbqa/models/generative_qa/fusion_in_decoder/natural_questions/pytorch_model.bin",
        "subdir": "{MODEL_PATH}"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/kbqa/models/generative_qa/fusion_in_decoder/natural_questions/optimizer.pth.tar",
        "subdir": "{MODEL_PATH}"
      }
    ]
  }
}
